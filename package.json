{
  "name": "empower-takehome",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "barrel": "barrelsby --delete -l all -d ./src",
    "db:init": "bun run src/migrations/init.ts",
    "dev": "bun --hot src/server.ts",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/server.ts"
  },
  "dependencies": {
    "barrelsby": "^2.8.1",
    "jose": "^6.1.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router": "^7.9.5",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}